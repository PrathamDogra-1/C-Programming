#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,pos;
    scanf("%d",&n);
    int list[n+1];
    for(int i=0;i<n;i++){
        scanf("%d",&list[i]);
    }
    scanf("%d",&pos);
    if(pos<1 || pos>n){
        printf("Invalid position");
    }
    else{

    for(int a=pos-1;a<n-1;a++){
        list[a]=list[a+1];
    }
    n--;
    for(int i=0;i<n;i++){
        printf("%d ",list[i]);
    }
        }
    
    return 0;
}
