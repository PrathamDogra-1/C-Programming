#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(){
    int count=0;
    char str[1001];
    scanf("%[^\n]",str);
    for(int i=0;str[i]!='\0';i++){
        
        if(str[i]=='A' || str[i]=='E' || str[i]=='I' || str[i]=='O' || str[i]=='U' || str[i]=='a' || str[i]=='e' || str[i]=='i' || str[i]=='o' || str[i]=='u'  ){
            count++;
        }
    }
    printf("%d",count);
    return 0;
}
