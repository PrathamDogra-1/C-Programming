#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d ",&n);
    int arr1[n],arr2[n];
    int *ptr1=arr1,*ptr2=arr2;
    for(int i=0;i<n;i++){
        scanf("%d",(ptr1+i));
    }
    for(int i=0;i<n;i++){
        scanf("%d",(ptr2+i));
    }
    for(int i=0;i<n;i++){
        int temp=*(ptr1+i);
        *(ptr1+i)=*(ptr2+i);
        *(ptr2+i)=temp;
    }
    for(int i=0;i<n;i++){
        printf("%d ",*(ptr1+i));
    }
    printf("\n");
    for(int i=0;i<n;i++){
        printf("%d ",*(ptr2+i));
    }
    return 0;
}
