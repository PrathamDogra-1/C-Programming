#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(){
    int counte=0,countc=0;
    char str[1001];
    scanf("%[^\n]",str);
    for(int i=0;str[i]!='\0';i++){
        
        if(str[i]=='A' || str[i]=='E' || str[i]=='I' || str[i]=='O' || str[i]=='U' || str[i]=='a' || str[i]=='e' || str[i]=='i' || str[i]=='o' || str[i]=='u'  ){
            counte++;
        }
        else if ((str[i] >= 'A' && str[i] <= 'Z') || (str[i] >= 'a' && str[i] <= 'z')) {
            countc++;
        }
    }
    printf("%d %d",counte,countc);
    return 0;
}
