#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int arr[n];

    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    int largest = arr[0];
    int second = -10001; 
    for (int i = 1; i < n; i++) {
        if (arr[i] > largest)
            largest = arr[i];
    }
    for (int i = 0; i < n; i++) {
        if (arr[i] != largest && arr[i] > second)
            second = arr[i];
    }
    if (second == -10001)
        printf("No second largest element");
    else
        printf("%d", second);

    return 0;
}
