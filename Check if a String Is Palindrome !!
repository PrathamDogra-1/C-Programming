#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>

int main(){
    char str[1001],revstr[1001];
    scanf("%[^\n]",str);
    int k=0;
    for(int i=0;str[i]!='\0';i++){
        str[i]=toupper(str[i]);
    }
    for(int i=strlen(str)-1;i>=0;i--){
        revstr[k++] = str[i];
    }
    revstr[k]='\0';
    if(strcmp(revstr,str)==0){
        printf("Palindrome");
    }
    else{
        printf("Not Palindrome");
    }
    
    return 0;
}
